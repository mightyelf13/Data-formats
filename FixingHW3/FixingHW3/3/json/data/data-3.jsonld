{
  "@context": {
    "@vocab": "http://example.org/schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",

    "countries": { "@container": "@set" },
    "cinemas":   { "@container": "@set" },

    "name":      { "@container": "@language" },
    "code":      "http://example.org/schema#code",

    "capacity":  { "@type": "xsd:integer" },
    "luxury":    { "@type": "xsd:boolean" },

    "phones":    { "@container": "@set" },
    "location":  { "@type": "@id" }
  },

  "countries": [
    { "id": "country-us", "name": { "en": "United States" }, "code": "US" },
    { "id": "country-uk", "name": { "en": "United Kingdom" }, "code": "GB" },
    { "id": "country-fr", "name": { "en": "France" }, "code": "FR" }
  ],

  "cinemas": [
    {
      "id": "cinema-imax-london",
      "name": { "en": "IMAX London" },
      "capacity": 500,
      "luxury": true,
      "phones": ["+441234567890", "+442345678901"],
      "location": "country-uk"
    },
    {
      "id": "cinema-amc-los-angeles",
      "name": { "en": "AMC Los Angeles" },
      "capacity": 350,
      "luxury": false,
      "phones": [],
      "location": "country-us"
    },
    {
      "id": "cinema-le-grand-paris",
      "name": { "en": "Le Grand Paris" },
      "capacity": 220,
      "luxury": true,
      "phones": ["+330987654321"],
      "location": "country-fr"
    }
  ]
}

