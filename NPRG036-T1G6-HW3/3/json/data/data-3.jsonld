{
  "@context": {
    "@base": "https://data.example.org/",
    "@vocab": "http://example.org/vocabulary/",
    "ex": "http://example.org/vocabulary/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",
    "type": "@type",
    
    "isoCode": "ex:isoCode",

    "label": {
      "@id": "rdfs:label",
      "@container": "@language"
    },

    "name": {
      "@id": "foaf:name",
      "@container": "@language"
    },

    
    "capacity": { "@id": "ex:capacity", "@type": "xsd:integer" },
    "isLuxury": { "@id": "ex:isLuxury", "@type": "xsd:boolean" },

    "phones": {
      "@id":        "ex:phone",
      "@type":      "@id",
      "@container": "@set"
    },
    "locatedIn": { "@id": "ex:locatedIn", "@type": "@id" }
  },

  "@graph": [
    { "id": "country/us", "type": "Country", "label": { "en": "United States" },  "isoCode": "US" },
    { "id": "country/uk", "type": "Country", "label": { "en": "United Kingdom" }, "isoCode": "GB" },
    { "id": "country/fr", "type": "Country", "label": { "en": "France" },         "isoCode": "FR" },

    {
      "id": "cinema/imax-london",
      "type": "Cinema",
      "name": { "en": "IMAX London" },
      "capacity": 500,
      "isLuxury": true,
      "phones": [
        "tel:+441234567890",
        "tel:+442345678901"
      ],
      "locatedIn": "country/uk"
    },
    {
      "id": "cinema/amc-la",
      "type": "Cinema",
      "name": { "en": "AMC Los Angeles" },
      "capacity": 350,
      "isLuxury": false,
      "phones": [],
      "locatedIn": "country/us"
    },
    {
      "id": "cinema/le-grand-paris",
      "type": "Cinema",
      "name": { "en": "Le Grand Paris" },
      "capacity": 220,
      "isLuxury": true,
      "phones": [
        "tel:+330987654321"
      ],
      "locatedIn": "country/fr"
    }
  ]
}