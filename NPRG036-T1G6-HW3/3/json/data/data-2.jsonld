{
  "@context": {
    "@base": "https://data.example.org/",
    "@vocab": "http://example.org/vocabulary/",
    "ex": "http://example.org/vocabulary/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",
    "type": "@type",
    
    "isoCode": "ex:isoCode",

    "label": {
      "@id": "rdfs:label",
      "@container": "@language"
    },

    "name": {
      "@id": "foaf:name",
      "@container": "@language"
    },

    "stageName": {
      "@id": "ex:stageName",
      "@container": "@language"
    },

    "birthDate":   { "@id": "ex:birthDate",   "@type": "xsd:date" },

    "debutYear":   { "@id": "ex:debutYear",   "@type": "xsd:gYear" },
    "bornIn":      { "@id": "ex:bornIn",      "@type": "@id" }
  },

  "@graph": [
    { "id": "country/us", "type": "Country", "label": { "en": "United States" },  "isoCode": "US" },
