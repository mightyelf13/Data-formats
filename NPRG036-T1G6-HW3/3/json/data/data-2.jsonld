      "debutYear": 1991,
      "bornIn": "country/us"
    },
    {
      "id": "person/amy-adams",
      "type": "Actor",
      "name": { "en": "Amy Adams" },
      "birthDate": "1974-08-20",
      "stageName": { "en": "Amy Adams" },
      "debutYear": 1995,
      "bornIn": "country/us"
    },
    {
      "id": "person/gal-gadot",
      "type": "Actor",
      "name": { "en": "Gal Gadot" },
      "birthDate": "1985-04-30",
      "stageName": { "en": "Gal Gadot" },
      "debutYear": 2007,
      "bornIn": "country/us"
    }
  ]
}